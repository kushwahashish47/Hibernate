package com.rays.inheritance;

public class payment {

	private int id;
	private int amount;
	private String paymentTypr;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public String getPaymentTypr() {
		return paymentTypr;
	}
	public void setPaymentTypr(String paymentTypr) {
		this.paymentTypr = paymentTypr;
	}
	
}
